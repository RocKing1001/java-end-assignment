<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1">
    <HBox prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <VBox spacing="8.0" fx:id="navMenu">
            <Button prefWidth="100.0" text="Dashboard" onAction="#navDashboard"/>
            <Button prefWidth="100.0" text="Create order" onAction="#navCreateOrder"/>
            <Button prefWidth="100.0" text="Product inventory" onAction="#navProductInventory"/>
            <Button prefWidth="100.0" text="Order history" onAction="#navOrderHistory"/>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="40.0"/>
            </padding>
        </VBox>
        <Pane fx:id="currentPane"/>
    </HBox>
</AnchorPane>
